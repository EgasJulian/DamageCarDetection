{
    "version": 2,
    "builds": [
      {
        "src": "main.py", 
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb", 
          "runtime": "python3.12" 
        }
      },
      {
          "src": "frontend/**", 
          "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/analyze-damage/(.*)", 
        "dest": "main.py"
      },
      {
        "src": "/(.*)", 
        "dest": "/frontend/$1"
      }
  
    ]
  }